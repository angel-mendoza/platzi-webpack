{
  "name": "curso-de-webpack",
  "version": "1.0.0",
  "description": "curso de webpack de platzi",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js --output bundle.js --mode development",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "extract": "webpack --config ./extract/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:multy": "webpack --config ./multy-entry-points/webpack.config.js",
    "watch": "webpack --config ./webpack-dev-server/webpack.config.js --watch",
    "start": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js --open",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js",
    "build:images": "webpack --config ./url-loader-images/webpack.config.js",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js",
    "build:video": "webpack --config ./url-loader-video/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.5",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
